#include "RPattern.h"

void GeneratePattern(int output[12][3]) {
    int patterns[12][3] = {
        {1, 2, 1}, {1, 2, 3}, {1, 3, 1}, {1, 3, 2},
        {2, 1, 2}, {2, 1, 3}, {2, 3, 1}, {2, 3, 2},
        {3, 1, 2}, {3, 1, 3}, {3, 2, 1}, {3, 2, 3}
    };

    for (int i = 0; i < 12; i++) {
       for(int j = 0; j<3; j++){
    	   output[i][j] = patterns[i][j]
       }
    return 0;
}

